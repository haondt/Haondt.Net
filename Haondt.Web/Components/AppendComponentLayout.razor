@using Haondt.Web.Core.Extensions
@using Haondt.Web.Extensions
@inherits Haondt.Web.Core.Components.Component
@code {
    [Parameter]
    public List<Haondt.Web.Core.Components.IComponent> Components { get; set; } = [];
}

@foreach(var component in Components)
{
    <ObjectComponent Component="component"/>
} 

